{
  "name": "so",
  "version": "1.0.0",
  "description": "一个简洁且好看的搜索导航",
  "background": {
    "scripts": ["background/bg.js","background/log.js"]
  },
	"permissions": [
    "contextMenus",
		"tabs"
  ],
	"options_page": "options/options.html",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
	"browser_action": {
		"default_icon": "images/logo128.png", 
		"default_title": "so",
		"default_popup": "popup/index.html"
	},
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://*/*"],
			"js": ["content/log1.js"]
		}
	],
  "manifest_version": 2
}
